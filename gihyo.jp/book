<div id="main">
<div id="bookTopBanner" class="booktop-banner" data-max="1">
<a href="/book/series?s=%E3%81%BE%E3%81%AA%E3%81%B3%E3%81%AE%E3%81%9A%E3%81%8B%E3%82%93" data-idx="0" class="is-active"><img src="/assets/images/ad/816x298/2019/manabi-2.jpg" srcset="/assets/images/ad/816x298/2019/manabi-2.jpg 1x, /assets/images/ad/816x298/2019/manabi-2@2x.jpg 2x" alt="まなびのずかん"></a>
</div>
<script type="text/javascript">
(function(){
  var rotateBanner = {
    wrap: {},
    max: 0,
    timer: 0,
    lock: false,

    init: function() {
      this.wrap = document.querySelector('.booktop-banner');
      if (document.body.classList.contains('smartphone')) {
        setTimeout(function(wrap){
          var zoom = wrap.offsetWidth / 816;
          wrap.style.zoom = zoom;
          wrap.style.marginBottom = (15 / zoom) + 'px';
          if (document.body.offsetWidth < 500) {
            wrap.style.marginTop = '-' + (28 / zoom) + 'px';
          }
        }, 500, this.wrap);
      }

      this.max = Number(this.wrap.getAttribute('data-max'));

      if (this.max < 2) {
        return false;
      }

      var span = document.createElement('span');
      span.classList.add('prev');
      span.title = '前のバナー';
      this.wrap.appendChild(span);
      span.addEventListener('click', function(e){
        rotateBanner.prev();
      });
      span = document.createElement('span');
      span.classList.add('next');
      span.title = '次のバナー';
      this.wrap.appendChild(span);
      span.addEventListener('click', function(e){
        rotateBanner.next();
      });
      span = void 0;

      return true;
    },

    prev: function() {
      if (this.lock) {
        return;
      }

      this.lock = true;

      if (this.timer) {
        clearTimeout(this.timer);
      }

      var target = this.wrap.querySelector('.is-active');
      var idx = Number(target.getAttribute('data-idx'));
      var prv = idx - 1;
      if (prv === -1) {
        prv = this.max - 1;
      }
      var nobj = this.wrap.querySelector('a[data-idx="' + prv + '"]');
      nobj.classList.add('dleft');
      setTimeout(function(nobj, target){
        target.classList.add('in');
        nobj.classList.add('is-active');
        nobj.classList.remove('dleft');
        setTimeout(function(target){
          target.classList.remove('is-active');
          target.classList.remove('in');
          target = void 0;
          rotateBanner.lock = false;
          rotateBanner.timer = setTimeout(function(){rotateBanner.prev()}, 12000);
        } ,1000, target);
        target = void 0;
        nobj = void 0;
      }, 10, nobj, target);
      target = void 0;
      nobj = void 0;
    },

    next: function() {
      if (this.lock) {
        return;
      }

      this.lock = true;

      if (this.timer) {
        clearTimeout(this.timer);
      }

      var target = this.wrap.querySelector('.is-active');
      var idx = Number(target.getAttribute('data-idx'));
      var nxt = idx + 1;
      if (nxt === this.max) {
        nxt = 0;
      }
      var nobj = this.wrap.querySelector('a[data-idx="' + nxt + '"]');
      nobj.classList.add('dright');
      setTimeout(function(nobj, target){
        target.classList.add('out');
        nobj.classList.add('is-active');
        nobj.classList.remove('dright');
        setTimeout(function(target){
          target.classList.remove('is-active');
          target.classList.remove('out');
          target = void 0;
          rotateBanner.lock = false;
          rotateBanner.timer = setTimeout(function(){rotateBanner.next()}, 12000);
        } ,1000, target);
        target = void 0;
        nobj = void 0;
      }, 10, nobj, target);
      target = void 0;
      nobj = void 0;
    },
  }

  if (rotateBanner.init()) {
    rotateBanner.timer = setTimeout(function(){rotateBanner.next()}, 12000);
  }
})();
</script>

